<configuration>
  <system.web>
    <compilation targetFramework="4.0" debug="true"/>
    <pages enableEventValidation="false" validateRequest="false"/>
    <sessionState mode="InProc" timeout="30"/>
    <httpRuntime requestPathInvalidCharacters="" />
  </system.web>
  <system.webServer>
    <defaultDocument>
      <files>
        <add value="login.aspx"/>
      </files>
    </defaultDocument>
  </system.webServer>
</configuration>